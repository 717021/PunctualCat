{
  "name": "punctual-cat",
  "version": "1.0.0",
  "description": "校园铃声自动播放系统是一款为中小学校播音工作室打造的一款可自动播放铃声、音乐的一款软件.",
  "main": "./main.js",
  "scripts": {
    "start": "npm run dev",
    "test": "npm run unit",
    "dev": "cross-env NODE_ENV=development webpack --progress --color --colors --watch --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "one-build": "cross-env NODE_ENV=production webpack --progress --color --colors --config build/webpack.prod.conf.js",
    "run-electron": "electron ./dist/development",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder build",
    "pack-win32": "electron-builder build --ia32",
    "pack-win64": "electron-builder build --x64"
  },
  "keywords": [],
  "author": "梦欤",
  "license": "ISC",
  "config": {
    "arch": "ia32",
    "registry": "https://registry.npm.taobao.org"
  },
  "build": {
    "productName": "PunctualCat",
    "appId": "com.imengyu.punctualcat",
    "copyright": "© 2019 梦欤 版权所有",
    "directories": {
      "output": "dist-build",
      "app": "./dist/production"
    },
    "win": {
      "icon": "src/assets/images/logo.ico",
      "target": [
        "dir"
      ]
    }
  },
  "dependencies": {
    "@types/jquery": "^3.3.31",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "chalk": "^3.0.0",
    "element-ui": "^2.11.1",
    "html-withimg-loader": "^0.1.16",
    "jquery": "^3.4.1",
    "log4js": "^6.0.0",
    "nedb": "^1.8.0",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-class-component": "^7.1.0",
    "vue-material": "^1.0.0-beta-11",
    "vue-property-decorator": "^8.2.1",
    "vue-slicksort": "^1.1.3",
    "webpack": "^4.39.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bindings": "^1.5.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^6.1.4",
    "electron-builder": "^22.1.0",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.5",
    "eslint": "^6.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "nan": "^2.14.0",
    "node-notifier": "^6.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.2.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.19.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "uglify-js": "^3.7.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-obfuscator": "^0.18.5"
  }
}
